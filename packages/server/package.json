{
  "name": "@surgioproject/gateway",
  "license": "MIT",
  "version": "0.0.1",
  "repository": "https://github.com/surgioproject/gateway",
  "author": "Yihang Li <me@royli.dev>",
  "bugs": {
    "url": "https://github.com/surgioproject/gateway/issues"
  },
  "homepage": "https://surgio.royli.dev",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "run-s build:*",
    "build:server": "nest build",
    "build:client": "react-scripts build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:client": "react-scripts start",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config server/test/jest-e2e.json",
    "test:client": "react-scripts tes)"
  },
  "dependencies": {
    "@nestjs/common": "^6.8.0",
    "@nestjs/core": "^6.8.0",
    "@nestjs/platform-express": "^6.8.0",
    "@nestjs/platform-fastify": "^6.10.14",
    "got": "^10.2.1",
    "http-errors": "^1.7.3",
    "nunjucks": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "surgio": "^1.11.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.1",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.8.0",
    "@now/node": "^1.3.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/koa": "^2.11.0",
    "@types/koa-router": "^7.0.42",
    "@types/node": "^10",
    "@types/nunjucks": "^3.1.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/supertest": "^2.0.8",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "autoprefixer": "^9.7.3",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "tailwindcss": "^1.1.4",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^6.0.1",
    "type-fest": "^0.8.1",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
